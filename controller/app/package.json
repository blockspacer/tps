    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config webpack.config.js && cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config app.webpack.config.js",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config webpack.config.js && cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config app.webpack.config.js && node treedys-controller.js",
